{
  "collectionName": "components_seo_defaults",
  "info": {
    "displayName": "SEO Defaults",
    "description": "Default SEO settings for the website"
  },
  "options": {},
  "attributes": {
    "metaTitle": {
      "type": "string",
      "maxLength": 60,
      "required": true
    },
    "metaDescription": {
      "type": "text",
      "maxLength": 160,
      "required": true
    },
    "keywords": {
      "type": "text"
    },
    "ogImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "siteName": {
      "type": "string",
      "required": true
    },
    "twitterHandle": {
      "type": "string"
    }
  }
}
